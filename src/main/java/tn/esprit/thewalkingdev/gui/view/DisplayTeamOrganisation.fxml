                  <Font size="30.0" />
               </font>
            </Label>
            <Button fx:id="addMemberBtnId" layoutX="215.0" layoutY="420.0" mnemonicParsing="false" onAction="#AddMemberBtnAction" opacity="0.8" prefHeight="38.0" prefWidth="151.0" stylesheets="@../../css/button.css" text="Add member" />
