<AnchorPane fx:id="pane" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.thewalkingdev.gui.controller.DisplayTeamOrganisationController">
   <children>
      <AnchorPane layoutX="-7.0" prefHeight="489.0" prefWidth="815.0">
         <children>
            <ImageView fx:id="imageViewId" fitHeight="489.0" fitWidth="874.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Images/background9.png" />
               </image>
            </ImageView>
            <TableView fx:id="table" layoutX="78.0" layoutY="128.0" opacity="0.8" prefHeight="276.0" prefWidth="659.0" stylesheets="@../../css/button.css">
               <columns>
                  <TableColumn fx:id="lastNameCol" prefWidth="146.0" text="Last Name" />
                  <TableColumn fx:id="firstNameCol" prefWidth="138.0" text="First Name" />
                  <TableColumn fx:id="roleCol" prefWidth="133.0" text="Role" />
