<AnchorPane fx:id="pane" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.thewalkingdev.gui.controller.DisplayTeamsController">
   <children>
      <ImageView fitHeight="497.0" fitWidth="824.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Images/TeamBackground.png" />
