            <Button fx:id="sponsoringBtn" layoutX="408.0" layoutY="249.0" mnemonicParsing="false" onAction="#SponsoringBtnAction" opacity="0.0" prefHeight="228.0" prefWidth="396.0" text="sponsoring" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
