      </ImageView>
      <AnchorPane prefHeight="489.0" prefWidth="815.0">
         <children>
            <Button fx:id="logisticBtn" layoutY="2.0" mnemonicParsing="false" onAction="#LogisticBtnAction" opacity="0.0" prefHeight="244.0" prefWidth="407.5" text="logistic" />
